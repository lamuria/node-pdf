        d.once('error', function (err) {\n                t.ifError(err);\n                t.end();\n            });\n\n            d.add(t);\n            d.run(function () {\n                t.end = once(function () {\n                    t.done();\n                });\n                t.notOk = function notOk(ok, message) {\n                    return (t.ok(!ok, message));\n                };\n\n                tester.call(self, t);\n            });\n        };\n    },\n\n    getLog: function (name, stream, level) {\n        return (bunyan.createLogger({\n            level: (process.env.LOG_LEVEL || level || 'fatal'),\n            name: name || process.argv[1],\n            stream: stream || process.stdout,\n            src: true,\n            serializers: restify.bunyan.serializers\n        }));\n    },\n\n\n    get dtrace() {\n        var dtp;\n        try {\n            var d = require('dtrace-provider');\n            dtp = d.createDTraceProvider('restifyUnitTest');\n        } catch (e) {\n            dtp = null;\n        }\n\n        return (dtp);\n    }\n};","nodes":{"0":{"loc":{"start":{"line":7,"column":0},"end":{"line":7,"column":31}},"id":0},"1":{"loc":{"start":{"line":7,"column":13},"end":{"line":7,"column":30}},"id":1},"2":{"loc":{"start":{"line":9,"column":0},"end":{"line":9,"column":31}},"id":2},"3":{"loc":{"start":{"line":9,"column":13},"end":{"line":9,"column":30}},"id":3},"4":{"loc":{"start":{"line":10,"column":0},"end":{"line":10,"column":27}},"id":4},"5":{"loc":{"start":{"line":10,"column":11},"end":{"line":10,"column":26}},"id":5},"6":{"loc":{"start":{"line":12,"column":0},"end":{"line":12,"column":35}},"id":6},"7":{"loc":{"start":{"line":12,"column":14},"end":{"line":12,"column":34}},"id":7},"8":{"loc":{"start":{"line":17,"column":0},"end":{"line":99,"column":2}},"id":8},"9":{"loc":{"start":{"line":20,"column":8},"end":{"line":33,"column":10}},"id":9},"10":{"loc":{"start":{"line":21,"column":12},"end":{"line":21,"column":36}},"id":10},"11":{"lo